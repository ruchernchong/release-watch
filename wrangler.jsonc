{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "release-watch",
  "main": "src/index.ts",
  "compatibility_date": "2025-12-11",
  "compatibility_flags": ["nodejs_compat"],
  "observability": {
    "enabled": true
  },
  "triggers": {
    "crons": ["*/15 * * * *"]
  },
  "routes": [
    {
      "pattern": "releasewatch.dev",
      "custom_domain": true
    }
  ]
}
